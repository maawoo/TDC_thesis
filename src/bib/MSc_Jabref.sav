@Article{Nativi2017,
  author      = {Stefano Nativi and Paolo Mazzetti and Max Craglia},
  title       = {A view-based model of data-cube to support big earth data systems interoperability},
  year        = {2017},
  volume      = {1},
  abstract    = {Big  Earth  Data-Cube  infrastructures  are  becoming  more  and  more  popular  to  provide Analysis  Ready  Data,  especially  for  managing  satellite  time  series.  These  infrastructures build  on  the  concept  of  multidimensional  data  model  (data  hypercube)  and  are  complex  systems engaging different disciplines and expertise. For this reason, their interoperability capacity has become a challenge in the Global Change and Earth System science domains. To address this challenge, there is a pressing need in the community to reach a widely agreed definition  of  Data-Cube  infrastructures  and  their  key  features.  In  this  respect,  a  discussion  has  started  recently  about  the  definition  of  the  possible  facets  characterizing    Data-Cube  in  the  Earth  Observation  domain.  This  manuscript  contributes  to  such  debate  by  introducing  a  view-based  model  of  Earth  Data-Cube  systems  to  design its infrastructural architecture and content schemas, with the final  goal  of  enabling  and  facilitating  interoperability.  It  introduces  six modeling views, each of them is described according to: its main concerns,  principal  stakeholders,  and  possible  patterns  to  be  used.  The manuscript onsiders the Business Intelligence experience with Data Warehouse and multidimensional “cubes” along with the more recent and analogous development in the Earth Observation domain, and  puts  forward  a  set  of  interoperability  recommendations  based  on the modeling views.},
  comment     = {- 2017 schon wieder etwas veraltet und andere Paper sind in dem Kontext etwas relevanter...
- Trägt generell aber zur Diskussion bei, dass gewisse Dinge im Zusammenhang mit EO Data Cubes besser definiert werden sollten

Key points:

- Data-Cube systems must be separated into distinct sections, such that each section addresses a separate concern.
- A concern is a set of requirements and constraints that affect the system design and represent a  stakeholder  needs.  By  applying  the  separation  of  concerns  pattern,  Data-Cubes  can  be  represented in a way that is manageable and comprehensible by a range of different business and technical stakeholders. The benefits are multiple including: maintainability, extensibility, interoperability.
- Content views:
	- Semantic
	- Geometry
	- Encoding
- Cyber-infrastructure views:
	- Interaction/Interface
	- Interconnection/Platform
	- Composition/Ecosystem},
  doi         = {10.1080/20964471.2017.1404232},
  groups      = {DataCubes - Impact / Use / Definitions},
  incitefulid = {50366670},
  keywords    = {prio2, skimmed},
  magid       = {2771417949},
  priority    = {prio2},
  readstatus  = {skimmed},
}

@Article{Sudmanns2020,
  author      = {Martin Sudmanns and Dirk Tiede and Stefan Lang and Helena Bergstedt and Georg Trost and Hannah Augustin and Andrea Baraldi and Thomas Blaschke},
  journal     = {International Journal of Digital Earth},
  title       = {Big Earth data: disruptive changes in Earth observation data management and analysis?},
  year        = {2020},
  volume      = {13},
  abstract    = {Turning Earth observation (EO) data consistently and systematically into valuable global information layers is an ongoing challenge for the EO community. Recently, the term ‘big Earth data’ emerged to describe massive EO datasets that confronts analysts and their traditional workflows with a range of challenges. We argue that the altered circumstances must be actively intercepted by an evolution of EO to revolutionise their application in various domains. The disruptive element is that analysts and end-users increasingly rely on Web-based workflows. In this contribution we study selected systems and portals, put them in the context of challenges and opportunities and highlight selected shortcomings and possible future developments that we
consider relevant for the imminent uptake of big Earth data.},
