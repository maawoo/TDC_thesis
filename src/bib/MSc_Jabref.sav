  month      = {jul},
  publisher  = {{IEEE}},
  abstract   = {The recent step change in the velocity and volume of Earth Observation data acquisition is both an opportunity and a threat. Richer more timely data supports the development of new EO derived products and opportunities for economic and environmental benefit. Ironically, this change also threatens the innovation pipeline and the ability for organizations to exploit the outcomes as they are swamped with data management and computation challenges. The CSIRO is using the Open Data Cube in combination with a new business engagement model via its Earth Analytics Industry Innovation Hub to address these challenges and accelerate the uptake of Earth analytics innovation in Australia.},
  doi        = {10.1109/igarss.2018.8519170},
