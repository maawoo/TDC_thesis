@Article{Lewis2016,
  author      = {Adam Lewis and Leo Lymburner and Matthew B. J. Purss and Brendan P. Brooke and Benjamin J. K. Evans and Alex Ip and Arnold G. Dekker and James R. Irons and Stuart Minchin and Norman Mueller and Simon Oliver and Dale Roberts and Barbara Ryan and Medhavy Thankappan and Robert Woodcock and Lesley Wyborn},
  journal     = {International Journal of Digital Earth},
  title       = {Rapid, high-resolution detection of environmental change over continental scales from satellite data – the Earth Observation Data Cube},
  year        = {2016},
  volume      = {9},
  abstract    = {The effort and cost required to convert satellite Earth Observation (EO)data  into  meaningful  geophysical  variables  has  prevented  thesystematic analysis of all available observations. To overcome theseproblems, we utilise an integrated High Performance Computing andData environment to rapidly process, restructure and analyse theAustralian Landsat data archive. In this approach, the EO data areassigned to a common grid framework that spans the full geospatialand temporal extent of the observations–the EO Data Cube. Thisapproach  is  pixel-based  and  incorporates  geometric  and  spectralcalibration and quality assurance of each Earth surface reflectancemeasurement. We demonstrate the utility of the approach with rapidtime-series mapping of surface water across the entire Australiancontinent using 27 years of continuous, 25 m resolution observations.Our preliminary analysis of the Landsat archive shows how the EOData Cube can effectively liberate high-resolution EO data from theircomplex sensor-specific data structures and revolutionise our ability tomeasure environmental change.},
  comment     = {Lewis et al. (2017) !},
  doi         = {10.1080/17538947.2015.1111952},
  groups      = {DataCubes - ODC General},
  incitefulid = {46919419},
  issue       = {1},
  keywords    = {prio3, skimmed},
  magid       = {2236111634},
  priority    = {prio3},
  readstatus  = {skimmed},
}

@Article{Siqueira2019,
