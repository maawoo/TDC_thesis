@Article{Giuliani2019,
  author     = {Gregory Giuliani and Joan Mas{\'{o}} and Paolo Mazzetti and Stefano Nativi and Alaitz Zabala},
  journal    = {Data},
  title      = {Paving the Way to Increased Interoperability of Earth Observations Data Cubes},
  year       = {2019},
  month      = {jul},
  number     = {3},
  pages      = {113},
  volume     = {4},
  abstract   = {Earth observations data cubes (EODCs) are a paradigm transforming the way users interact with large spatio-temporal Earth observation (EO) data. It enhances connections between data, applications and users facilitating management, access and use of analysis ready data (ARD). The ambition is allowing users to harness big EO data at a minimum cost and effort. This significant interest is illustrated by various implementations that exist. The novelty of the approach results in different innovative solutions and the lack of commonly agreed definition of EODC. Consequently, their interoperability has been recognized as a major challenge for the global change and Earth system science domains. The objective of this paper is preventing EODC from becoming silos of information; to present how interoperability can be enabled using widely-adopted geospatial standards; and to contribute to the debate of enhanced interoperability of EODC. We demonstrate how standards can be used, profiled and enriched to pave the way to increased interoperability of EODC and can help delivering and leveraging the power of EO data building, efficient discovery, access and processing services.},
  comment    = {- Einige Probleme von EODCs, die in Zukunft gelöst werden sollten, werden beschrieben (Interoperability auf verschiedenen Leveln)
- Differenzierung zwischen Data Cubes und Cloud-basierten Lösungen (z.B. GEE)
- Definition von EODCs wird behandelt
	- 'Datacube Manifesto' und 'The six faces of a datacube' werden zitiert aber es wird nicht unbedingt gesagt, dass das jetzt die beste Definition von EODCs ist
- Insgesamt wahrscheinlich wichtig für die Diskussion und das ein oder andere auch für's Intro!},
  doi        = {10.3390/data4030113},
  file       = {:/home/marco/STUDIUM/MA/Research/DataCube/Giuliani2019 - Paving the Way to Increased Interoperability of Earth Observations Data Cubes.pdf:PDF},
  groups     = {MA - Intro, MA - Discussion, DataCubes - Impact / Use / Definitions},
  keywords   = {prio1, read},
  priority   = {prio1},
  publisher  = {{MDPI} {AG}},
  readstatus = {read},
}

@Article{Giuliani2019a,
  author     = {Gregory Giuliani and Gilberto Camara and Brian Killough and Stuart Minchin},
  journal    = {Data},
  title      = {Earth Observation Open Science: Enhancing Reproducible Science Using Data Cubes},
  year       = {2019},
  month      = {nov},
  number     = {4},
  pages      = {147},
  volume     = {4},
  abstract   = {Earth Observation Data Cubes (EODC) have emerged as a promising solution to efficiently and effectively handle Big Earth Observation (EO) Data generated by satellites and made freely and openly available from different data repositories. The aim of this Special Issue, “Earth Observation Data Cube”, in Data, is to present the latest advances in EODC development and implementation, including innovative approaches for the exploitation of satellite EO data using multi-dimensional (e.g., spatial, temporal, spectral) approaches. This Special Issue contains 14 articles covering a wide range of topics such as Synthetic Aperture Radar (SAR), Analysis Ready Data (ARD), interoperability, thematic applications (e.g., land cover, snow cover mapping), capacity development, semantics, processing techniques, as well as national implementations and best practices. These papers made significant contributions to the advancement of a more Open and Reproducible Earth Observation Science, reducing the gap between users’ expectations for decision-ready products and current Big Data analytical capabilities, and ultimately unlocking the information power of EO data by transforming them into actionable knowledge.},
  comment    = {- Intro-Paper zum MDPI-Data-Special zum Thema EODCs
- Entsprechend wenig nützlicher Inhalt, aber zu Beginn paar Sätze die nützlich für mein Intro sein könnten},
  doi        = {10.3390/data4040147},
  file       = {:/home/marco/STUDIUM/MA/Research/DataCube/Giuliani2019a - Earth Observation Open Science_ Enhancing Reproducible Science Using Data Cubes.pdf:PDF},
  groups     = {MA - Intro, DataCubes - Impact / Use / Definitions},
  keywords   = {prio3, read},
  priority   = {prio3},
  publisher  = {{MDPI} {AG}},
  readstatus = {read},
}

